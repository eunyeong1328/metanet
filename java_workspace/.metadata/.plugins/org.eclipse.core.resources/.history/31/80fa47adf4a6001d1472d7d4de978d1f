package kosa.video;

public class main {

	public static void main(String[] args) {
		GeneralMember g = new GeneralMember();
		g.setId("aaa");
		g.setName("홍길동");
		g.setAddress("동탄");
		
		g.setId("aaa");
		g.setName("홍길동");
		g.setAddress("동탄");
		
		Video v = new Video();
		v.setSno(1);
		v.setTitle("트랜스포머3");
		v.setActor("서봉수");
		
		v.setSno(2);
		v.setTitle("쿵푸펜더2");
		v.setActor("지성민");

		SpecialMember s = new SpecialMember();
		s.setPoint(10);
		s.setPoint(20);
		
		g.show();
		v.show();
		s.show();
		
		GeneralMember g = new GeneralMember
				("aaa","홍길동","동탄", new Video(1,"트랜스포머3","서봉수"),new SpecialMember(10));
		
		
	}

}
