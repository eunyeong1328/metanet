package kosa.assignment;

public class problem1 {
	
	public static void main(String[] args) {
		int n = 5;
		int i = 1;
		int j = 1;
			
		for(i=1; i<n+1; i++) {
			System.out.println("iÀÇ °ªÀº" + i);
			for(j=0; j<i; j++) {
				System.out.print(j + (j-i)+ 1 + " ");
			}
			System.out.println();
		}
	}
	
	
	
}
